{
  "mcpServers": {
    "todoist": {
      "env": {
        "TODOIST_API_TOKEN": "your-api-token-here"
      },
      "args": [
        "-y",
        "@abhiz123/todoist-mcp-server"
      ],
      "disabled": false,
      "command": "npx"
    },
    "fetch": {
      "args": [
        "mcp-server-fetch"
      ],
      "command": "uvx"
    }
  }
}